{
  "name": "@caokejian/nuwa-components",
  "version": "1.0.13",
  "description": "女娲组件库",
  "main": "index.js",
  "files": [
    "**/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.+(js|ts|jsx|tsx)\"",
    "dev": "cross-env NODE_ENV=development webpack serve -c build/webpack.dev.js",
    "up:dev": "cross-env NODE_ENV=development node build/upload.js",
    "up:prod": "cross-env NODE_ENV=production node build/upload.js",
    "build:dev": "cross-env NODE_ENV=production webpack -c build/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production webpack -c build/webpack.prod.js",
    "publish:dev": "cross-env NODE_ENV=development bash bin/publish.bash",
    "publish:prod": "cross-env NODE_ENV=production bin/publish.bash"
  },
  "keywords": [
    "React",
    "ui组件",
    "typescript",
    "webpack"
  ],
  "homepage": "https://github.com/CaoKejian/NuWa",
  "author": {
    "name": "CaoKejian",
    "email": "CaoKejian@foxmail.com",
    "url": "https://github.com/CaoKejian"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "core-js": "^3.37.1",
    "create-nuwa-template": "^1.0.0",
    "dotenv-webpack": "^8.1.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0"
  },
  "engines": {
    "pnpm": ">=7.0.0"
  },
  "engine-strict": true,
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^9.2.0",
    "@storybook/react": "^8.1.1",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.0.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.1.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.4",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.8.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  }
}
